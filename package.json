{
  "name": "astrox",
  "type": "module",
  "version": "0.0.2",
  "scripts": {
    "dev:frontend": "astro dev",
    "dev:backend": "cargo watch -x run",
    "start": "cargo run --release",
    "build": "astro check && astro build && cargo build --release",
    "build:pages": "astro check && astro build",
    "format": "prettier --write '**/*.{astro,html,js,jsx,md,mdx,svelte,ts,tsx,vue}' && cargo fmt",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,astro}' && cargo clippy",
    "launch": "npm run build && npm start",
    "astro": "astro"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/tailwind": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "astro": "^5.5.5",
    "eslint": "^8.57.1",
    "eslint-plugin-astro": "^0.34.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.37.4",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@astrojs/react": "^4.2.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.29.2",
    "lucide-react": "^0.563.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.10"
  }
}
